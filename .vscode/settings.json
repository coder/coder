{
  "files.exclude": {
    "**/node_modules": true
  },
  "go.lintTool": "golangci-lint",
  "go.lintFlags": ["--fast"],
  "go.lintOnSave": "package",
  "go.coverOnSave": true,
  "go.coverageDecorator": {
    "type": "gutter",
    "coveredHighlightColor": "rgba(64,128,128,0.5)",
    "uncoveredHighlightColor": "rgba(128,64,64,0.25)",
    "coveredBorderColor": "rgba(64,128,128,0.5)",
    "uncoveredBorderColor": "rgba(128,64,64,0.25)",
    "coveredGutterStyle": "blockgreen",
    "uncoveredGutterStyle": "blockred"
  },
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "database/query.sql",
        "cmd": "make gen"
      }
    ]
  },
  "cSpell.words": [
    "coderd",
    "coderdtest",
    "codersdk",
    "drpc",
    "drpcconn",
    "drpcmux",
    "drpcserver",
    "fatih",
    "goleak",
    "hashicorp",
    "httpmw",
    "isatty",
    "Jobf",
    "kirsle",
    "manifoldco",
    "mattn",
    "moby",
    "nhooyr",
    "nolint",
    "nosec",
    "oneof",
    "parameterscopeid",
    "promptui",
    "protobuf",
    "provisionerd",
    "provisionersdk",
    "retrier",
    "sdkproto",
    "stretchr",
    "tfexec",
    "tfstate",
    "unconvert",
    "xerrors",
    "yamux"
  ]
}
